programa{
	entero N;
	lee N;

	mientras N<0 o N==0 {
		escribe "Ingresa un número mayor o igual a 1:";
		lee N;	
	}

	escribe "Los "+N+" primeros términos de la serie de Fibonacci son:";
	
	si N==2 entonces{
		escribe 1;
	}
	si N>2 entonces{
		entero fibo1=1;
		entero fibo2=1;
		entero fibonacci;	
		entero i=0;
		para i desde 3 hasta N{
			fibonacci = fibo1 + fibo2;
			escribe fibonacci;
			fibo1 = fibo2;
			fibo2 = fibonacci;
		}
	}
	escribe "fin del programa";
}
